{
  "name": "ioea",
  "version": "0.0.1",
  "description": "The goal of this project is to make the easiest, fastest, and most painless file system.",
  "author": "drsh(1340260725@qq.com)",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@automapper/classes": "^2.0.0",
    "@automapper/core": "^2.0.0",
    "@nestjs/common": "^7.6.7",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.4",
    "@nestjs/platform-express": "^7.6.4",
    "@nestjs/swagger": "^4.8.0",
    "express-form-data": "^2.0.16",
    "minio": "^7.0.18",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@automapper/types": "^2.0.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.6.4",
    "@types/express": "^4.17.8",
    "@types/express-form-data": "^2.0.1",
    "@types/jest": "^26.0.15",
    "@types/minio": "^7.0.7",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.16",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "jest-each": "^27.1.1",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
